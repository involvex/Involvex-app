{
  "expo": {
    "name": "involvex",
    "slug": "involvex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "involvex",
    "githubUrl": "https://github.com/involvex/involvex-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.involvex.involvex",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.involvex.involvex",
      "scheme": "involvex"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "startUrl": "http://192.168.178.69:8081"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-dev-client",
        {
          "http": true,
          "webSocket": true,
          "launchWithDevTools": true,
          "debuggerHost": "192.168.178.69:8081",
          "debuggerPort": 9222,
          "host": "192.168.178.69:8081"
        }
      ],
      [
        "expo-dev-menu",
        {
          "launchWithDevTools": true,
          "devClient": true,
          "debuggerHost": "192.168.178.69:8081",
          "debuggerPort": 9222,
          "host": "192.168.178.69:8081"
        }
      ],

      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "sounds": [],
          "mode": "development"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {
        "origin": "http://192.168.178.69:8081",
        "scheme": "involvex",
        "hostname": "192.168.178.69",
        "port": 8081,
        "http": true,
        "devClient": true
      },
      "eas": {
        "projectId": "c1ba12c1-38c1-4f48-ba5c-8c9b0c1287cd"
      }
    }
  }
}
